<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane fx:id="root" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="name.azzurite.mcserver.view.ConsoleView">
   <children>
      <TextArea fx:id="consoleTextArea" editable="false" minHeight="60.0" minWidth="100.0" prefHeight="60.0" prefWidth="100.0">
         <font>
            <Font name="Courier New" size="12.0" />
         </font></TextArea>
      <TextField fx:id="input" prefHeight="25.0" GridPane.rowIndex="1" />
   </children>
   <columnConstraints>
      <ColumnConstraints hgrow="ALWAYS" />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints valignment="TOP" vgrow="ALWAYS" />
      <RowConstraints valignment="BOTTOM" vgrow="NEVER" />
   </rowConstraints>
</GridPane>
